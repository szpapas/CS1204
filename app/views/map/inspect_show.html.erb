<script>
	function showTips( tips, left, height, time ){
    var windowWidth  = document.documentElement.clientWidth;
    var tipsDiv = '<div class="tipsClass">' + tips + '</div>';
		$( 'div.tipsClass' ).remove();
		
    $( 'body' ).append( tipsDiv );
    $( 'div.tipsClass' ).css({
        'top'       : height + 'px',
        'left'      : left + 'px',
        'position'  : 'absolute',
        'padding'   : '3px 5px',
        'background': '#FBB117',
        'font-size' : '16px',
        'margin'    : '0 auto',
        'text-align': 'center',
        'width'     : 'auto',
        'color'     : '#fff',
        'opacity'   : '1.0'
    }).show();
    setTimeout( function(){$( 'div.tipsClass' ).remove();}, ( time * 1000 ) );
  };

	$("p").hover(function(e){
		if (this.textContent.length > 10) {
			left = 20;
		} else {
			left = e.clientX;
		}
		showTips( this.textContent, left , e.clientY, 3 );
	});
	
	
</script>



